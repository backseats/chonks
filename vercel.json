{
  "functions": {
    "pages/api/og.tsx": {
      "memory": 1024,
      "maxDuration": 10
    },
    "pages/api/og-fallback.tsx": {
      "memory": 1024,
      "maxDuration": 10
    },
    "pages/api/og-puppeteer.tsx": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/og-fallback",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/api/og-puppeteer",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
